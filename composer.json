{
    "name": "polygon-io/api",
    "description": "polygon.io php client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Prugne",
            "email": "julien@webeleon.dev"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.4",
        "amphp/websocket-client": "^0.2.4"
    },
    "autoload": {
        "psr-4": {
            "PolygonIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PolygonIO\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "phan/phan": "^3.1"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
